# Python program to implement PUSH and POP operation subject to exception

 
 
stack = []
 

stack.append('a')
stack.append('b')
stack.append('c')
try:
   raise IndexError
except Exception as e:
   excepName = type(e)
    
 
print('initial stack')
print(stack)
print('\nElements poped from stack:')
print(stack.pop())
print(stack.pop())
print(stack.pop())
print(stack)
