#program to create a binary tree and insert at least 12 nodes into it through user input values from console

class node:
    def __init__(self,data):
        self.data=data
        self.right= None
        self.left= None
class BinaryTree:
    def __init__(self):
        self.root=None
        self.root_right = None
        self.root_left= None
ob1=BinaryTree()
